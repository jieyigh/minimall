	}


	public void setCustomer_member_mobile(String customer_member_mobile) {
		this.customer_member_mobile = customer_member_mobile;
	}


	public Long getStore_id() {
		return store_id;
	}


	public void setStore_id(Long store_id) {
		this.store_id = store_id;
	}


